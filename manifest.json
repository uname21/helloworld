{
    "name": "Hello world",
    "version": "2.6",
    "description": "",
    "permissions": [
        "activeTab", "storage", "declarativeContent", "tabs", "background"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "externally_connectable": {
      "matches": ["*://*.hy3237222.xyz/*"]
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
      "commands": {
        "toggle-feature-foo": {
          "suggested_key": {
            "default": "Ctrl+Shift+Y",
            "mac": "Command+Shift+Y"
          },
          "description": "Toggle feature foo"
        },
        "clear": {
            "suggested_key": {
              "default": "Ctrl+Shift+U",
              "mac": "Command+Shift+U"
            },
            "description": "clear"
          }
    },
    "manifest_version": 2
}